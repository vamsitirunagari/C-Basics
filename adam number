#include<stdio.h>
int main()
{
    int r,n,sq=0,rev_sq=0,rem,reverse=0,rev=0;
    scanf("%d",&n);
    sq=n*n;
    while(n!=0)
    {
        r=n%10;
        rev=(rev*10)+r;
        n/=10;
    }
    rev_sq=rev*rev;
    while(sq!=0)
    {
        rem=sq%10;
        reverse=(reverse*10)+rem;
        sq/=10;
        
        
    }
    if(reverse==rev_sq)
    {
        printf("True");
    }
    else
    {
        printf("False");
    }
}
